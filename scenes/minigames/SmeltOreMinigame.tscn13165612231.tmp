[sub_resource type="GDScript" id="GDScript_d8ggs"]
script/source = "class_name PullLever
extends TextureButton
signal pull_completed(strength: float)

@export var max_pull_distance: float = 300.0

@export var chain_node: TextureRect

var is_dragging: bool = false
var original_button_pos: Vector2
var original_chain_pos: Vector2
var drag_start_y: float

func _ready():
	original_button_pos = position
	
	if chain_node:
		original_chain_pos = chain_node.position
	else:
		push_error(\"Lỗi: Node 'Chain' chưa được gán vào PullLever script!\")

func _gui_input(event: InputEvent):

	if event is InputEventMouseButton and event.button_index == MOUSE_BUTTON_LEFT:
		if event.is_pressed():
			is_dragging = true
			drag_start_y = get_global_mouse_position().y
			get_viewport().set_input_as_handled()
		else:
			if is_dragging:
				is_dragging = false
				
				var drag_delta = get_global_mouse_position().y - drag_start_y
				var strength = clampf(drag_delta / max_pull_distance, 0.0, 1.0)
				
				pull_completed.emit(strength)
				
				var tween = create_tween()
				tween.set_parallel(true) 
				
				tween.tween_property(self, \"position\", original_button_pos, 0.2).set_trans(Tween.TRANS_SPRING)
				
				if chain_node:
					tween.tween_property(chain_node, \"position\", original_chain_pos, 0.2).set_trans(Tween.TRANS_SPRING)
				
				get_viewport().set_input_as_handled()

	if event is InputEventMouseMotion:
		if is_dragging:
			var drag_delta = get_global_mouse_position().y - drag_start_y
			
			var clamped_delta = clampf(drag_delta, 0.0, max_pull_distance)
			
			position.y = original_button_pos.y + clamped_delta
			
			if chain_node:
				chain_node.position.y = original_chain_pos.y + clamped_delta
			
			get_viewport().set_input_as_handled()
"

