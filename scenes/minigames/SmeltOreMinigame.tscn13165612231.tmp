script/source = "extends Panel

# --- Kéo các node vào đây trong Inspector ---
# 1. Kéo node AnimationPlayer của cái nồi
@export var cauldron_anim_player: AnimationPlayer

# 2. Kéo node PullLever (TextureButton)
@export var pull_lever: PullLever


# --- Các biến để tính toán ---
var anim_duration: float = 0.0
var original_lever_y: float = 0.0
var max_pull_distance: float = 0.0

func _ready():
	# --- A. Lấy thông tin Animation ---
	if cauldron_anim_player == null or pull_lever == null:
		push_error(\"Lỗi: Vui lòng kéo 'cauldron_anim_player' và 'pull_lever' vào Inspector.\")
		return
		
	if cauldron_anim_player.has_animation(\"tilt\"):
		# Lấy tổng thời lượng của animation (ví dụ: 1.943 giây)
		anim_duration = cauldron_anim_player.get_animation(\"tilt\").length
		
		# QUAN TRỌNG: Bắt đầu bằng cách \"play\" và \"pause\" ngay
		# Điều này cho phép .seek() hoạt động chính xác
		cauldron_anim_player.play(\"tilt\")
		cauldron_anim_player.seek(0.0, true) # \"true\" là để update ngay
		cauldron_anim_player.pause()
	else:
		push_error(\"Lỗi: Không tìm thấy animation 'tilt' trong AnimationPlayer.\")
		
	original_lever_y=pull_lever.position.y
	max_pull_distance=pull_lever.max_pull_distance


func _process(delta: float):
	# Nếu chưa sẵn sàng, không làm gì cả
	if anim_duration == 0.0:
		return

	# --- LOGIC LIÊN KẾT THỜI GIAN THỰC ---
	
	# 1. Lấy vị trí Y hiện tại của cần kéo
	var current_lever_y = pull_lever.position.y
	
	# 2. Tính toán \"strength\" (sức mạnh 0.0 -> 1.0)
	# inverse_lerp: Tính xem \"current_lever_y\" đang ở % bao nhiêu
	# trên quãng đường từ \"original_lever_y\" đến \"vị trí kéo tối đa\"?
	var strength = inverse_lerp(original_lever_y, original_lever_y + max_pull_distance, current_lever_y)
	
	# Kẹp giá trị (clamp) để đảm bảo nó luôn từ 0.0 đến 1.0
	strength = clampf(strength, 0.0, 1.0)

	# 3. Tính toán thời gian (time) trong animation
	var target_time = strength * anim_duration
	
	# 4. Tua (seek) animation đến đúng thời điểm
	# Đây là dòng \"ma thuật\" liên kết mọi thứ
	cauldron_anim_player.seek(target_time)
"

